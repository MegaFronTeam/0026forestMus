mixin top-nav() 
	// start top-nav
	+b.top-nav.block-with-lazy
		.container
			.row
				.col-lg-auto.col
					+e.logo
						a(href="/")
						img(src="img/svg/logo.svg" alt="")
				.col-lg.order-lg-0.order-last
					.menu-mobile.menu-mobile--js
						.menu-mobile__inner
							+menu-toggle()
							+mnu()
				.col-auto
					+e.btn.btn-sm(href="#sAccess") Начать обучение
				.col-auto.d-lg-none
					+menu-toggle()
	// end top-nav